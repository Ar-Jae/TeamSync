# Copy to .env and fill in values for local development

# JWT secret used to sign auth tokens
JWT_SECRET=your_jwt_secret_here

# MongoDB connection string (Atlas or local)
MONGO_URI=mongodb://localhost:27017/teamsync

# Server port
PORT=4000

# NODE_ENV=development or production
NODE_ENV=development

# Optional: external WebSocket URL (if different)
YSERVER_URL=ws://localhost:1234
